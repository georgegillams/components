{
  "name": "gg-components",
  "version": "0.0.5",
  "description": "Reusable transpiled React components",
  "repository": {
    "type": "git",
    "url": "git://github.com/georgegillams/gg-components.git"
  },
  "author": "George Gillams",
  "license": "MIT",
  "scripts": {
    "release": "release-it",
    "transpile": "npm run pretranspile && npm run transpile:js && npm run transform:js && npm run transpile:sass",
    "transpile:js": "babel src -d dist --copy-files",
    "transform:js": "node scripts/transform-js-scss-css-imports.js",
    "transpile:sass": "node scripts/build-sass-in-place.js",
    "clean": "npm run test:clean && npm run build:clean",
    "dev": "npm run start:dev",
    "fix:adapter-core-5": "cp ./scripts/adapter_core5.js node_modules/webrtc-adapter/src/js/adapter_core5.js",
    "fix:package-lock": "sed -i '' -e 's/http:\\/\\//https:\\/\\//g' $(find . -name 'package-lock.json')",
    "lint": "npm run lint:js && npm run lint:scss",
    "lint:js": "eslint . --ext .js,.jsx",
    "lint:js:fix": "eslint . --ext .js,.jsx --fix",
    "lint:scss": "stylelint 'packages/**/*.scss' --syntax scss",
    "transpile:clean": "rimraf ./dist",
    "pretranspile": "npm run transpile:clean",
    "pretest": "npm run test:clean",
    "prettier": "prettier --config .prettierrc --write \"**/*.js\"",
    "node-sass": "node-sass",
    "start:dev": "cross-env echo \"NEED TO START STORYBOOK HERE\"",
    "test": "cross-env NODE_ENV=test jest --coverage",
    "test:clean": "rimraf ./coverage",
    "test:watch": "cross-env NODE_ENV=test jest --watchAll"
  },
  "main": "dist/index.js",
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ],
    "*.scss": [
      "stylelint --syntax scss"
    ]
  },
  "dependencies": {
    "babel-polyfill": "6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "bibtex-parse-js": "0.0.24",
    "chalk": "^2.3.2",
    "compression": "1.7.2",
    "conventional-redux": "^1.0.3",
    "cookie-parser": "^1.4.3",
    "cross-env": "^5.1.6",
    "express": "4.16.3",
    "express-session": "^1.15.6",
    "fontfaceobserver": "2.0.13",
    "funnies": "^2.0.0",
    "heroku-ssl-redirect": "0.0.4",
    "history": "4.7.2",
    "hoist-non-react-statics": "2.5.0",
    "immutable": "3.8.2",
    "invariant": "2.2.4",
    "ip": "1.1.5",
    "js-regression": "^1.0.10",
    "lodash": "^4.17.11",
    "minimist": "1.2.0",
    "moment": "^2.22.2",
    "nodemailer": "^4.6.8",
    "pretty-error": "^2.1.1",
    "prop-types": "^15.6.1",
    "react": "^16.4.0",
    "react-component-academic-reference": "^1.0.5",
    "react-cookies": "^0.1.0",
    "react-dom": "^16.4.0",
    "react-helmet": "5.2.0",
    "react-loadable": "^5.4.0",
    "react-loading": "^2.0.3",
    "react-qr-reader": "^2.2.1",
    "react-qr-svg": "^2.2.1",
    "react-redux": "5.0.7",
    "react-responsive-modal": "^3.4.0",
    "react-router-dom": "^4.3.1",
    "react-router-redux": "5.0.0-alpha.6",
    "react-stripe-elements": "^2.0.3",
    "redis": "^2.8.0",
    "redux": "3.7.2",
    "redux-immutable": "4.0.0",
    "redux-saga": "0.16.0",
    "reselect": "3.0.1",
    "safe-compare": "^1.1.3",
    "sanitize.css": "4.1.0",
    "scroll-into-view": "^1.9.3",
    "socket.io": "^2.1.1",
    "stripe": "^6.23.1",
    "styled-components": "^4.3.1",
    "tensorscript": "^1.2.2",
    "warning": "^4.0.1",
    "wget-improved": "^3.0.2",
    "whatwg-fetch": "2.0.4",
    "wink-perceptron": "^2.0.0",
    "youtube-embed-video": "^1.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.3.4",
    "@babel/preset-env": "^7.3.4",
    "add-asset-html-webpack-plugin": "2.1.3",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "8.2.3",
    "babel-jest": "^24.3.1",
    "babel-loader": "7.1.4",
    "babel-plugin-add-module-exports": "^1.0.0",
    "babel-plugin-dynamic-import-node": "1.2.0",
    "babel-plugin-react-transform": "3.0.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
