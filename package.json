{
  "name": "@george-gillams/components",
  "version": "26.0.1",
  "description": "Reusable transpiled React components",
  "repository": {
    "type": "git",
    "url": "git://github.com/georgegillams/gg-components.git"
  },
  "author": "George Gillams",
  "license": "MIT",
  "main": "./index.js",
  "scripts": {
    "prerelease": "npm run transpile",
    "release": "babel-node scripts/release.js",
    "release:alpha": "yarn release --alpha",
    "transpile": "yarn pretranspile && yarn transpile:js && yarn transform:js && yarn transpile:sass && yarn transpile:copy-tokens && yarn transpile:remove-stories",
    "transpile:js": "babel src -d dist --copy-files",
    "transform:js": "babel-node scripts/transform-js-scss-css-imports.js",
    "transpile:sass": "babel-node scripts/build-sass-in-place.js",
    "transpile:copy-tokens": "cp -R ./src/Tokens/* ./dist/Tokens/",
    "transpile:remove-stories": "find ./dist -name \"stories.js\" -exec rm {} \\;",
    "transpile:js:live": "yarn transpile:js --watch",
    "clean": "npm run test:clean && npm run build:clean",
    "dev": "npm run start",
    "fix:adapter-core-5": "cp ./scripts/adapter_core5.js node_modules/webrtc-adapter/src/js/adapter_core5.js",
    "fix:package-lock": "sed -i '' -e 's/http:\\/\\//https:\\/\\//g' $(find . -name 'package-lock.json')",
    "lint": "npm run lint:js && npm run lint:sass",
    "lint:js": "eslint src --fix && eslint test --fix && eslint scripts --fix",
    "lint:sass": "echo \"SKIPPING stylelint 'src/**/*.scss' --syntax scss\"",
    "transpile:clean": "rimraf ./dist",
    "pretranspile": "npm run transpile:clean",
    "pretest": "npm run test:clean",
    "prettier": "prettier --config .prettierrc --write \"**/*.js\"",
    "sass": "sass",
    "storybook": "start-storybook -p 9001",
    "build": "build-storybook -c .storybook -o build --quiet",
    "build:for-deploy:clean": "rm -rf deploy/build",
    "build:for-deploy": "npm run build:for-deploy:clean && npm run build && cp -R build deploy/ && cp -R config deploy/",
    "build:test": "npm run build",
    "backstopjs:test": "node ./node_modules/http-server/bin/http-server ./build -p 9001 -a 127.0.0.1 & (sleep 2 && babel-node ./scripts/backstopjs/backstop.js)",
    "backstopjs:test:allow-failure": "node ./node_modules/http-server/bin/http-server ./build -p 9001 -a 127.0.0.1 & (sleep 2 && babel-node ./scripts/backstopjs/backstop.js --allowFailure)",
    "start": "npm run storybook",
    "prod": "http-server build",
    "start:aws": "AWS=true npm run prod",
    "test": "npm run test:lint && npm run test:jest",
    "test:jest": "cross-env jest --coverage",
    "test:jest:debug": "node --inspect node_modules/.bin/jest --runInBand --watchAll",
    "test:lint": "npm run lint:sass",
    "test:clean": "rimraf ./coverage",
    "test:watch": "cross-env NODE_ENV=test jest --watchAll"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ],
    "*.scss": [
      "stylelint --syntax scss"
    ]
  },
  "dependencies": {
    "a11y-focus-scope": "^1.1.3",
    "date-fns": "^2.28.0",
    "lodash.memoize": "^4.1.2",
    "lodash.throttle": "^4.1.1",
    "recompose": "^0.30.0",
    "youtube-embed-video": "^1.2.1"
  },
  "peerDependencies": {
    "prop-types": "^15.7.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "styled-components": "^5.3.5"
  },
  "devDependencies": {
    "@babel/cli": "^7.13.14",
    "@babel/core": "^7.13.14",
    "@babel/node": "^7.13.13",
    "@babel/plugin-proposal-class-properties": "^7.13.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.13.8",
    "@babel/plugin-proposal-private-methods": "^7.13.0",
    "@babel/preset-env": "^7.13.12",
    "@babel/preset-flow": "^7.13.13",
    "@babel/preset-react": "^7.13.13",
    "@babel/register": "^7.13.14",
    "@storybook/addon-a11y": "^6.4.20",
    "@storybook/addon-actions": "^6.4.20",
    "@storybook/addon-backgrounds": "^6.4.20",
    "@storybook/addon-docs": "^6.4.20",
    "@storybook/addon-knobs": "^6.4.0",
    "@storybook/addon-measure": "^6.5.3",
    "@storybook/addon-outline": "^6.5.3",
    "@storybook/addon-postcss": "^2.0.0",
    "@storybook/addon-viewport": "^6.4.20",
    "@storybook/react": "^6.4.20",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "12.1.5",
    "add-asset-html-webpack-plugin": "3.2.0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^27.3.1",
    "babel-loader": "^8.2.2",
    "babel-polyfill": "6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "backstopjs": "^5.3.7",
    "core-js": "^3.10.0",
    "cross-env": "^7.0.3",
    "css-loader": "4.3.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.6",
    "eslint": "^7.23.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-webpack": "^0.13.0",
    "eslint-plugin-flowtype": "^5.4.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.29.4",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint_d": "^9.1.2",
    "file-loader": "6.2.0",
    "http-server": "^0.12.3",
    "jest-cli": "^27.3.1",
    "node-sass-tilde-importer": "^1.0.2",
    "prettier": "^2.6.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-test-renderer": "^16.14.0",
    "rimraf": "3.0.2",
    "sass": "^1.43.2",
    "sass-lint": "^1.13.1",
    "sass-loader": "^10.1.1",
    "storybook-dark-mode": "^1.0.9",
    "style-loader": "^1.3.0",
    "styled-components": "^5.3.5",
    "styled-normalize": "^8.0.7",
    "stylelint": "^13.12.0",
    "webpack": "^5.72.0",
    "webpack-cli": "^4.9.2",
    "webpack-dev-middleware": "^3.7.3",
    "webpack-hot-middleware": "^2.25.0"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "heroku-run-build-script": true
}
