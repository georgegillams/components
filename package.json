{
  "name": "gg-components",
  "version": "0.0.48",
  "description": "Reusable transpiled React components",
  "repository": {
    "type": "git",
    "url": "git://github.com/georgegillams/gg-components.git"
  },
  "author": "George Gillams",
  "license": "MIT",
  "scripts": {
    "prerelease": "npm run transpile",
    "release": "release-it",
    "transpile": "npm run pretranspile && npm run transpile:js && npm run transform:js && npm run transpile:sass && npm run transpile:prune",
    "transpile:js": "babel src -d dist --copy-files",
    "transform:js": "node scripts/transform-js-scss-css-imports.js",
    "transpile:sass": "node scripts/build-sass-in-place.js",
    "transpile:prune": "find dist -name \"*.test.js\" -exec rm -rf {} \\; && find dist -name \"__snapshots__\" -exec rm -rf {} || true \\;",
    "clean": "npm run test:clean && npm run build:clean",
    "dev": "npm run start:dev",
    "fix:adapter-core-5": "cp ./scripts/adapter_core5.js node_modules/webrtc-adapter/src/js/adapter_core5.js",
    "fix:package-lock": "sed -i '' -e 's/http:\\/\\//https:\\/\\//g' $(find . -name 'package-lock.json')",
    "lint": "npm run lint:js && npm run lint:scss",
    "lint:js": "eslint . --ext .js,.jsx",
    "lint:js:fix": "eslint . --ext .js,.jsx --fix",
    "lint:scss": "stylelint 'packages/**/*.scss' --syntax scss",
    "transpile:clean": "rimraf ./dist",
    "pretranspile": "npm run transpile:clean",
    "pretest": "npm run test:clean",
    "prettier": "prettier --config .prettierrc --write \"**/*.js\"",
    "node-sass": "node-sass",
    "storybook": "start-storybook -p 9001",
    "build": "build-storybook -c .storybook -o storybook-dist",
    "backstopjs:test": "node ./node_modules/http-server/bin/http-server ./storybook-dist -p 9001 -a 127.0.0.1 & (sleep 2 && node ./scripts/backstopjs/backstop.js)",
    "start": "npm run storybook",
    "test": "cross-env NODE_ENV=test jest --coverage",
    "test:clean": "rimraf ./coverage",
    "test:watch": "cross-env NODE_ENV=test jest --watchAll"
  },
  "main": "dist/index.js",
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ],
    "*.scss": [
      "stylelint --syntax scss"
    ]
  },
  "dependencies": {
    "lodash": "^4.17.15",
    "moment": "^2.24.0",
    "prop-types": "^15.7.2",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-helmet": "5.2.1",
    "recompose": "^0.30.0",
    "scroll-into-view": "^1.9.7",
    "youtube-embed-video": "^1.2.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.5.5",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-proposal-object-rest-spread": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-flow": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.5.5",
    "@storybook/addon-a11y": "^5.2.1",
    "@storybook/addon-actions": "^5.2.1",
    "@storybook/addon-backgrounds": "^5.2.1",
    "@storybook/addon-knobs": "^5.2.1",
    "@storybook/addon-links": "^5.2.1",
    "@storybook/addon-viewport": "^5.2.1",
    "@storybook/react": "^5.2.1",
    "add-asset-html-webpack-plugin": "3.1.3",
    "babel-eslint": "^10.0.2",
    "babel-jest": "^24.9.0",
    "babel-loader": "^8.0.6",
    "babel-polyfill": "6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "backstopjs": "^4.3.2",
    "bpk-mixins": "^18.0.1",
    "bpk-tokens": "^27.7.0",
    "chalk": "^2.4.2",
    "circular-dependency-plugin": "5.2.0",
    "compression": "^1.7.4",
    "cross-env": "^6.0.0",
    "css-loader": "3.2.0",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "eslint": "^6.1.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-config-skyscanner-with-prettier": "^0.8.0",
    "eslint-import-resolver-webpack": "^0.11.1",
    "eslint-plugin-flowtype": "^4.2.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.14.3",
    "eslint-plugin-redux-saga": "^1.1.0",
    "eslint_d": "^8.0.0",
    "eventsource-polyfill": "0.9.6",
    "exports-loader": "0.7.0",
    "express": "4.17.1",
    "express-session": "^1.16.2",
    "file-loader": "4.2.0",
    "html-loader": "0.5.5",
    "html-webpack-plugin": "3.2.0",
    "http-server": "^0.11.1",
    "image-webpack-loader": "^6.0.0",
    "imports-loader": "0.8.0",
    "invariant": "2.2.4",
    "ip": "1.1.5",
    "jest-cli": "^24.9.0",
    "lint-staged": "^9.2.3",
    "minimist": "1.2.0",
    "node-plop": "^0.19.0",
    "node-sass": "^4.12.0",
    "node-sass-tilde-importer": "^1.0.2",
    "nodemon": "^1.19.1",
    "null-loader": "3.0.0",
    "plop": "2.4.0",
    "pretty-error": "^2.1.1",
    "react-router": "^5.0.1",
    "react-router-dom": "^5.0.1",
    "react-test-renderer": "^16.9.0",
    "release-it": "^12.3.5",
    "rimraf": "3.0.0",
    "sass": "^1.22.10",
    "sass-loader": "^8.0.0",
    "shelljs": "^0.8.3",
    "style-loader": "^1.0.0",
    "stylelint": "^11.0.0",
    "url-loader": "2.1.0",
    "webpack": "^4.39.2",
    "webpack-cli": "^3.3.7",
    "webpack-dev-middleware": "^3.7.0",
    "webpack-hot-middleware": "^2.25.0"
  },
  "jest": {
    "verbose": true,
    "testRegex": "src/.*\\.test\\.js$",
    "testPathIgnorePatterns": [
      "dist/"
    ],
    "moduleNameMapper": {
      "^.+\\.scss$": "<rootDir>/scripts/stubs/styleStub.js",
      "^.+\\.svg$": "<rootDir>/scripts/stubs/fileStub.js"
    }
  },
  "heroku-run-build-script": true
}
